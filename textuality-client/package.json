{
  "name": "textuality-client",
  "private": true,
  "scripts": {
    "start": "meteor npm install && MONGO_URL=mongodb://localhost:4001/meteor MONGO_OPLOG_URL=mongodb://localhost:4001/local meteor --port=4002 --settings ../settings.json",
    "start-prod": "./start-prod.sh",
    "deploy": "DEPLOY_HOSTNAME=galaxy.meteor.com meteor deploy textuality.meteorapp.com --settings ../settings.json",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer"
  },
  "dependencies": {
    "@babel/runtime": "~7.5.5",
    "@cloudinary/url-gen": "^1.12.0",
    "array-sort": "~1.0.0",
    "body-parser": "~1.19.0",
    "classnames": "~2.2.6",
    "cloudinary": "~1.41.0",
    "emoji-aware": "~3.0.5",
    "js-confetti": "^0.11.0",
    "luxon": "^3.4.3",
    "meteor-node-stubs": "~1.2.5",
    "pokemon": "^2.0.0",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "shuffle-array": "^1.0.1",
    "simpl-schema": "~3.4.1",
    "wouter": "~2.12.1"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    },
    "testModule": "tests/main.js"
  },
  "devDependencies": {
    "@babel/core": "~7.5.5",
    "@types/body-parser": "^1.19.4",
    "@types/luxon": "^3.3.4",
    "@types/node": "~20.8.10",
    "@types/react": "~18.2.33",
    "@types/react-dom": "~18.2.14",
    "@types/shuffle-array": "^1.0.4",
    "babel-plugin-module-resolver": "~3.2.0",
    "prettier": "~3.0.3",
    "typescript": "~5.2.2"
  }
}
